%stack_size = 4096

// The entrypoints for a service:
// - is_authorized: 0
// - refine: 5
// - accumulate: 10
// - on_transfer: 15
jump @is_authorized
fallthrough
fallthrough
fallthrough
jump @refine
fallthrough
fallthrough
fallthrough
jump @accumulate
fallthrough
fallthrough
fallthrough
jump @on_transfer
fallthrough
fallthrough
fallthrough

// RO data = 0/0 bytes
// RW data = 0/0 bytes
// Stack size = 8192 bytes
// Instructions = 9
// Code size = 20 bytes

pub @is_authorized:
  a0 = 0
  ret

pub @refine:
  a0 = 0x1
  ret

pub @accumulate:
  ret

pub @on_transfer:
  a0 = a0 << 0x1
  ret

pub @entry:
  a0 = 0
  ret
